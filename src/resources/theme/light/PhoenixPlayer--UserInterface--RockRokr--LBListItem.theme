#LBListItem:hover {
    border: none;
    padding: 0px;
    background-color: transparent;
}

#LBListItem:!hover {
    border: none;
    padding: 0px;
    background-color: transparent;
}


#LBListItem {
    border: 1px solid #FCFCFC;
    border-radius: 8px;
    padding: 0px;
    background-color: #FCFCFC;
}

#LBListItem[hoverState="active"] {
    border: 1px solid #336CFB;
    border-radius: 8px;
    padding: 0px;
    background-color: #336CFB;
}

