#TitleIconLabel {
    background: transparent;
}

#TitleIconLabel[iconName="TypeArtist"] {
    image: url(:/light/image/KEY_ARTISTS.svg);
}

#TitleIconLabel[iconName="TypeAlbums"] {
    image: url(:/light/image/KEY_ALBUMS.svg);
}

#TitleIconLabel[iconName="TypeSongs"] {
    image: url(:/light/image/KEY_SONGS.svg);
}

#TitleIconLabel[iconName="TypeGenres"] {
    image: url(:/light/image/KEY_GENRES.svg);
}

#TitleIconLabel[iconName="TypeFavorites"] {
    image: url(:/light/image/KEY_FAVORITES.svg);
}


#TitleIconLabel[iconName="TypeAddon"] {
    image: url(:/light/image/KEY_ADDON.svg);
}


#TitleIconLabel[iconName="TypeService"] {
    image: url(:/light/image/KEY_SERVICE.svg);
}

#TitleTextEdit {
    color: #1F2022;
    border: none;
    background-color:transparent;
}

#TitleTextEdit[hoverState="active"] {
    color: #FCFCFC;
    border: none;
    background-color:transparent;
}

#TitleTextEdit[editableStatus="active"] {
    color: #009aff;
}

#TitleTextEdit:focus[editableStatus="active"] {
    color: black;
    background-color: white;
    border-radius: 4.0px;
    border: 1px solid rgba(0, 0, 0, 0.08);
}

